{
  "name": "alleyhector-portfolio",
  "description": "Alley's Portfolio",
  "version": "1.1.1",
  "author": "Alley Hector <alley@qpdx.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alleyhector/portfolio"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "overrides": {
    "sharp": "latest"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "postinstall": "gatsby clean",
    "rebuild-sharp": "pnpm rebuild sharp"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "animejs": "^3.2.2",
    "babel-plugin-styled-components": "^2.1.4",
    "gatsby": "^5.15.0",
    "gatsby-background-image": "^1.6.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-offline": "^6.15.0",
    "gatsby-plugin-react-helmet": "^6.15.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-plugin-styled-components": "^6.15.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^7.15.0",
    "gatsby-remark-prismjs": "^7.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.30.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-background-slider": "^4.1.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-transition-group": "^4.4.5",
    "scrollreveal": "^4.0.9",
    "smooth-scroll": "^16.1.3",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@upstatement/eslint-config": "3.0.0",
    "@upstatement/prettier-config": "^1.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "gatsby-remark-code-titles": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2"
  }
}
